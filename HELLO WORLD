#include <iostream>
//using namespace std;
using std::cout;
using std::endl;

int main()
{
	/* Method 1 - without writing using namespace std;
	std::cout << "Hello World!" << std::endl; //namespace:: scope resolution operator
	std::cout << 10 + 50 << std::endl;
	int num = 10;
	std::cout << num;
	*/
	
	/* Method 2 - by writing using namespace std;
	cout << "Hello World!" << endl; //namespace:: scope resolution operator
	cout << 10 + 50 << endl;
	int num = 10;
	cout << num;
	*/
	
	/* Method 3 - selective inclusion of names in a namespace by writing using namespace::name;
	cout << "Hello World!" << endl; //namespace:: scope resolution operator
	cout << 10 + 50 << endl;
	int num = 10;
	cout << num;
	*/
	return 0;
}
